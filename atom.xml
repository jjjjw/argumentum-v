---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Argumentum Ad Verecundiam</title>
 <link href="http://www.argumentum-v.net/atom.xml" rel="self"/>
 <link href="http://www.argumentum-v.net/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.argumentum-v.net/</id>
 <author>
   <name>JJ Weber</name>
   <email>jj.weber@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.argumentum-v.net/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://argumentum-v.net/{{ post.url }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
